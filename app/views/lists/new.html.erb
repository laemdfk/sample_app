<h1>新規投稿</h1>
<%= form_with model:@list_new, method: :post do |f| %>

<%= f.label :title, "タイトル" %><br >
<%= f.text_field :title %><br >

<%= f.label :body,"本文" %><br >
<%= f.text_area :body %><br >

<%= f.label :image,"画像" %><br >
<%= f.file_field :image, accept: "image/*" %><br >

<%= f.submit "送信" %>

<% end %>